{
    "name": "gallery-plus",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "pnpm build-server && vite build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
        "fix:all": "npm run lint:fix && npm run format",
        "preview": "vite preview",
        "build-server": "tsc --noEmit -p tsconfig.server.json && tsup",
        "dev-server": "tsup --watch --onSuccess \"node server/dist/main.js\"",
        "run-server": "node server/dist/main.js",
        "start": "concurrently \"npm run dev-server\" \"npm run dev\""
    },
    "dependencies": {
        "@fastify/cors": "8.5.0",
        "@fastify/multipart": "^8.0.0",
        "@fastify/static": "^7.0.0",
        "@hookform/resolvers": "^5.2.2",
        "@radix-ui/react-dialog": "^1.1.15",
        "@tailwindcss/vite": "^4.1.10",
        "@tanstack/react-query": "^5.90.16",
        "axios": "^1.13.2",
        "classnames": "^2.5.1",
        "fastify": "^4.24.3",
        "nuqs": "^2.8.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.70.0",
        "react-router": "^7.12.0",
        "sonner": "^2.0.7",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.1.10",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/node": "^20.19.28",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react-swc": "^3.9.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.5.0",
        "jiti": "^2.6.1",
        "prettier": "^3.7.4",
        "tsup": "^8.5.0",
        "tw-animate-css": "^1.4.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.52.0",
        "vite": "^6.3.5",
        "vite-plugin-svgr": "^4.3.0"
    }
}
